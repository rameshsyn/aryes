{
  "name": "aryes",
  "version": "1.0.0",
  "description": "An Educational institution management system",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./server --watch ./server --exec babel-node",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "build": "npm run build:client && npm run build:server",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "clean:build": "rimraf build/*",
    "clean:node_modules": "rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "apollo-client": "^0.8.5",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "express": "4.14.0",
    "express-graphql": "^0.6.2",
    "express-react-views": "0.10.2",
    "express-session": "1.14.2",
    "express-validator": "3.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "font-awesome": "^4.7.0",
    "graphql": "^0.9.0",
    "graphql-tag": "^1.2.4",
    "immutability-helper": "^2.1.1",
    "moment": "^2.17.1",
    "mongoose": "4.7.6",
    "morgan": "1.7.0",
    "react": "15.4.1",
    "react-apollo": "^0.10.1",
    "react-dom": "15.4.1",
    "react-helmet": "3.2.3",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "sanitize.css": "^4.1.0",
    "semantic-ui-react": "^0.66.0",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-externals-plugin": "1.0.0",
    "webpack-hot-middleware": "2.15.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-es2015-classes": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.17.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "dotenv": "2.0.0",
    "file-loader": "0.9.0",
    "jest": "^18.1.0",
    "json-loader": "0.5.4",
    "node-sass": "4.1.1",
    "nodemon": "1.11.0",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "standard": "^8.6.0",
    "style-loader": "0.13.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/**/*.{js,jsx}"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/rameshsyn/aryes",
  "author": "Ramesh Syangtan <rameshsyangtan92@gmail.com>"
}
